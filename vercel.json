{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "staticFiles": {
    "/sitemap.xml": "public/sitemap.xml",
    "/robots.txt": "public/robots.txt"
  },
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
