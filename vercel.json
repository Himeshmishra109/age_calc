{
  "version": 2,

  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "age-calc-3lf5.vercel.app"
        }
      ],
      "destination": "https://clackmasterpro.online/$1",
      "permanent": true
    }
  ],

  "builds": [
    { "src": "app.py", "use": "@vercel/python" }
  ],

  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/app.py",
      "headers": {
        "Content-Type": "application/xml"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}

